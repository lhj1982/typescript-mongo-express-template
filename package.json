{
  "name": "jubenfan-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist/config",
    "test": "jest",
    "cp-files": "cp -r ./src/config/*.yaml ./dist/config/",
    "build": "npm run clean && npm run cp-files && tsc",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.json'",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "dev:watch": "NODE_ENV=development npm run generate && npm run prettier && npm run lint:fix && nodemon ./src/server.ts",
    "start:prod:single": "NODE_ENV=production node ./dist/server.js",
    "prod": "npm run build && npm run start:prod",
    "start:prod:daemon": "NODE_ENV=production npm run build && NODE_ENV=production pm2 start ./dist/server.js",
    "restart:prod:daemon": "NODE_ENV=production npm run build && NODE_ENV=production pm2 restart ./dist/server.js",
    "start:prod:cluster": "NODE_ENV=production npm run build && NODE_ENV=production pm2 start pm2.yaml",
    "restart:prod:cluster": "NODE_ENV=production npm run build && NODE_ENV=production pm2 restart pm2.yaml",
    "tsoa:generate": "tsoa routes && tsoa swagger",
    "generate": "tsoa swagger -c tsoa.json && tsoa routes -c tsoa.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkawk/typeScript-express.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkawk/typeScript-express/issues"
  },
  "homepage": "https://github.com/bkawk/typeScript-express#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-xml-bodyparser": "^0.3.1",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/mongodb": "^3.5.4",
    "@types/mongoose": "^5.7.8",
    "@types/node": "^12.7.3",
    "@types/swagger-ui-express": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "swagger-ui-express": "^4.1.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "helmet": "^3.18.0",
    "ioredis": "^4.16.0",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "5.7.x",
    "rate-limiter-flexible": "^1.1.1",
    "tsoa": "^2.4.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "xml2js": "^0.4.22"
  }
}
